<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqjtu.cosmetology.dao.ImageDao">
    <select id="queryImageByType" resultType="com.cqjtu.cosmetology.entity.Image">

        select * from image where imagestate=1 AND imagetype = #{type}
    </select>
</mapper>